{
    "manifest_version": 3,
    "name": "JumpCutTestVersion",
    "version": "1.0",
    "description": "Does shit, mind your own business.",
    "permissions": [
        "tabs",
        "<all_urls>",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "background": {
        "service_worker": "backgroundScript.js"
    },
    "content_scripts": [
        {
            "matches": ["https://www.google.com/search?q=*"],
            "js": ["contentScript.js"],
            "css": ["styles.css"]
        }
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}