{
    "manifest_version": 3,
    "name": "JumpCutTestVersion",
    "version": "1.0",
    "description": "Does shit, mind your own business.",
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "background": {
        "service_worker": "backgroundScript.js"
    },
    "content_scripts": [
        {
            "matches": ["*://www.google.com/search*", "*://www.google.com/webhp*", "*://*/*"],
            "js": ["contentScript.js"],
            "css": ["styles.css"]
        }
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}